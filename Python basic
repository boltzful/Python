def greeting(name, surname):
    return f"Cześć {name} {surname}!"

# Przykładowe użycie funkcji
name = "Jan"
surname = "Kowalski"
result = greeting(name, surname)
print(result)






def multiply_numbers(a, b):
    return a * b

# Przykładowe użycie funkcji
num1 = 5
num2 = 3
result = multiply_numbers(num1, num2)
print(result)



def is_even(number):
    return number % 2 == 0

# Przykładowe użycie funkcji
num = 6
result = is_even(num)

if result:
    print("Liczba parzysta")
else:
    print("Liczba nieparzysta")


def check_sum_greater_or_equal(num1, num2, num3):
    return num1 + num2 >= num3

# Przykładowe użycie funkcji
num1 = 5
num2 = 3
num3 = 7
result = check_sum_greater_or_equal(num1, num2, num3)
print(result)





def check_value_in_list(input_list, value):
    return value in input_list

# Przykładowe użycie funkcji
my_list = [1, 2, 3, 4, 5]
my_value = 3
result = check_value_in_list(my_list, my_value)
print(result)  # True

my_value = 6
result = check_value_in_list(my_list, my_value)
print(result)  # False




def process_lists(list1, list2):
    combined_list = list1 + list2  # łączenie list
    unique_list = list(set(combined_list))  # usuwanie duplikatów
    powered_list = [x ** 3 for x in unique_list]  # podnoszenie do potęgi 3
    return powered_list

# Przykładowe użycie funkcji
list1 = [1, 2, 3]
list2 = [3, 4, 5]
result = process_lists(list1, list2)
print(result)  # [1, 8, 27, 64, 125]
